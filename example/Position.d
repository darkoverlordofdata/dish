class Position {

    double x;
    double y;

    public void Position(double x, double y) {
        self.x = x;
        self.y = y;
    }
    
    public double getX() {
        return self.x;
    }
    public void setX(double x) {
        self.x = x;
    }
    public double getY() {
        return self.y;
    }
    public void setY(double y) {
        self.y = y;
    }

}